{
  "name": "gerald-the-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot that recommends computer game giveaways.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "ts-node-dev ./src/index.ts",
    "build": "tsc",
    "build:watch": "onchange \"src/**/*.ts\" -- npm run build",
    "test": "jest",
    "test:acceptance": "jest --testPathPattern=tests/acceptance",
    "test:unit": "jest --watchAll --testPathPattern=src",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparrowBrain/gerald-the-discord-bot.git"
  },
  "keywords": [
    "discord",
    "free"
  ],
  "author": "Qwx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SparrowBrain/gerald-the-discord-bot/issues"
  },
  "homepage": "https://github.com/SparrowBrain/gerald-the-discord-bot#readme",
  "devDependencies": {
    "@plant/http": "^1.0.4",
    "@plant/plant": "^2.5.0",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.7.23",
    "@types/source-map-support": "^0.5.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^27.5.1",
    "onchange": "^7.1.0",
    "serve": "^14.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^4.8.4",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3"
  },
  "dependencies": {
    "@plant/https": "^1.0.1",
    "aws-sdk": "2.1227.0",
    "cheerio": "1.0.0-rc.12",
    "discord.js": "14.5.0",
    "dotenv": "16.0.3",
    "express": "^4.18.1"
  }
}
